{
  "name": "swp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && mvn spring-boot:run",
    "dev:frontend": "vite",
    "start": "npm run dev:full",
    "dev:docker": "docker-compose up --build",
    "dev:docker-down": "docker-compose down",
    "backend:test": "cd backend && mvn test",
    "backend:build": "cd backend && mvn clean package",
    "check:health": "curl -f http://localhost:8080/actuator/health && curl -f http://localhost:5173",
    "logs:backend": "tail -f logs/backend.log",
    "logs:frontend": "tail -f logs/frontend.log"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.81.2",
    "antd": "^5.26.1",
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
